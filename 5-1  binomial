#include <stdio.h>

// Function to compute the Binomial coefficient using dynamic programming
int binomialCoefficient(int n, int k) {
    int C[n + 1][k + 1];
    int i, j;

    // Initialize all values to 0
    for (i = 0; i <= n; i++) {
        for (j = 0; j <= k; j++) {
            C[i][j] = 0;
        }
    }

    // Compute the Binomial coefficients
    for (i = 0; i <= n; i++) {
        for (j = 0; j <= i && j <= k; j++) {
            // Base cases
            if (j == 0 || j == i) {
                C[i][j] = 1;
            }
            // Recurrence relation based on provided conditions
            else {
                C[i][j] = C[i - 1][j - 1] + C[i - 1][j];
            }
        }
    }

    return C[n][k];
}

int main() {
    int n = 8;
    int k = 8;

    printf("Binomial Coefficient C(%d, %d) = %d\n", n, k, binomialCoefficient(n, k));

    return 0;
}
