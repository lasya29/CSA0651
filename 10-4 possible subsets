#include <stdio.h>
#include <stdlib.h>
#include <math.h>

// Function to print subsets
void printSubsets(int *nums, int numsSize) {
    int totalSubsets = pow(2, numsSize); // Total number of subsets (2^n)

    // Iterating over all possible subsets
    for (int i = 0; i < totalSubsets; i++) {
        printf("[");
        // Iterating over each element in nums
        for (int j = 0; j < numsSize; j++) {
            // Check if the j-th bit in i is set
            if (i & (1 << j)) {
                printf("%d ", nums[j]);
            }
        }
        printf("]\n");
    }
}

int main() {
    int nums[] = {1, 2, 3};
    int numsSize = sizeof(nums) / sizeof(nums[0]);

    printf("Output:\n");
    printSubsets(nums, numsSize);

    return 0;
}
